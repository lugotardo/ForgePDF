{
  "name": "lugotardo/forgepdf",
  "homepage": "https://github.com/lugotardo/ForgePDF",
  "description": "Forge Pdf Continuation of the piritual fpdf",
  "keywords": [
    "Forge",
    "fpdf",
    "pdf",
    "Forgepdf"
  ],
  "license": "Apache-2.0",
  "type": "library",
  "authors": [
    {
      "name": "Luan Costa",
      "email": "luan.gotardo.dev@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Fpdf\\": "src/Fpdf",
      "ForgePDF\\": "src/ForgePDF"
    }
  },
  "archive": {
    "exclude": [
      "original"
    ]
  },
  "extra": {
    "laravel": {
      "aliases": {
        "ForgePDF": "lugotardo\\ForgePDF"
      }
    }
  }
}
